{
    "name": "assinafy",
    "version": "1.0.2",
    "description": "TypeScript SDK for Assinafy API - Digital signature platform",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "test": "bun test",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "prepublishOnly": "bun run build",
        "release": "bun run build && bun test && bun lint && bun typecheck && bun run build && npm publish",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "assinafy",
        "assinatura-eletronica",
        "assinatura",
        "eletronica",
        "api-client",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gusnips/assinafy.git"
    },
    "bugs": {
        "url": "https://github.com/gusnips/assinafy/issues"
    },
    "homepage": "https://github.com/gusnips/assinafy#readme",
    "devDependencies": {
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "axios": "^1.6.0"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ]
}